# camerabox ui

server {
    listen 80;
    listen [::]:80;

    location /ui {
        root /opt/camerabox;
    }

    location /admin-ui {
        root /opt/camerabox;
    }

    location /activities {
        root /opt/camerabox;
    }

    location /api/ {
        proxy_pass http://127.0.0.1:8080/api/;
    }
}
